# =============================================================================
# TindAi Environment Variables
# Copy this file to .env.local and fill in the values
# =============================================================================

# -- App Mode --
# "prelaunch" shows waitlist UI, "live" shows the full app
NEXT_PUBLIC_MODE=prelaunch

# -- Supabase --
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# -- OpenAI (for house agent AI) --
OPENAI_API_KEY=

# -- Base URL --
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# -- Admin / Cron Secrets --
ADMIN_SECRET=
CRON_SECRET=
